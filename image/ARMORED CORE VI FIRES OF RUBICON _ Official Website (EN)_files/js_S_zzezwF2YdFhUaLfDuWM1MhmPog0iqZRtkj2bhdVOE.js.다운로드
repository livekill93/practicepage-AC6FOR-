"use strict";

(function ($, Drupal, once) {
  "use strict";

  Drupal.behaviors.bne_common_common = {
    attach: function attach(context) {
      var htmlElement = document.documentElement; // Header vars

      once('js-header', '#js-header').forEach(function (element) {
        var header = element;
        var headerNavButtonToggle = document.getElementById('js-header__nav-button-toggle');
        var headerAccountText = document.getElementById('js-header__account-text');
        var headerOverlay = document.getElementById('js-header__overlay');
        var isHeaderNavsOpened = false; // Scroll vars

        var previousKnownScrollPosition = window.scrollY;
        var lastKnownScrollPosition = 0;
        var tickingScroll = false; // Account menu vars

        var accountMenuButtonToggle = document.getElementById('js-header__account-button');
        var accountMenu = document.getElementById('js-header__account-menu');
        var isAccountMenuOpened = false;

        if (!header.classList.contains('js--header-small')) {
          document.addEventListener('scroll', headerOnScroll, false);
        } // END TODO REMOVE temp condition forcing small header


        function headerOnScroll() {
          lastKnownScrollPosition = window.scrollY;

          if (!tickingScroll) {
            window.requestAnimationFrame(function () {
              switchHeaderOnScroll(lastKnownScrollPosition);
              tickingScroll = false;
            });
            tickingScroll = true;
          }
        }

        function switchHeaderOnScroll(scrollYPos) {
          if (scrollYPos === 0) {
            htmlElement.classList.remove('js-html--header-small');
          } else {
            htmlElement.classList.add('js-html--header-small');
          }

          previousKnownScrollPosition = scrollYPos;
        }

        function toggleHeader() {
          if (accountMenu && accountMenu.classList.contains('js-header__account-menu--opened')) {
            toggleAccountMenu();
          }

          isHeaderNavsOpened = !isHeaderNavsOpened;
          htmlElement.classList.toggle('js-html-header--overflow-visible');
          headerNavButtonToggle.setAttribute("aria-expanded", isHeaderNavsOpened);
          htmlElement.classList.toggle('js-html-header--opened');
          htmlElement.classList.toggle('js-html--no-scroll');
          headerAccountText.classList.toggle('sr-only');
        }

        headerNavButtonToggle.addEventListener('click', function () {
          toggleHeader();
        });
        headerOverlay.addEventListener('click', function () {
          toggleHeader();
        });

        function toggleAccountMenu() {
          if (htmlElement.classList.contains('js-html-header--opened')) {
            toggleHeader();
          }

          isAccountMenuOpened = !isAccountMenuOpened;
          htmlElement.classList.toggle('js-html-header--overflow-visible');
          accountMenuButtonToggle.setAttribute("aria-expanded", isAccountMenuOpened);
          accountMenuButtonToggle.classList.toggle('js-header__account-button--opened');
          accountMenu.classList.toggle('js-header__account-menu--opened');
        }

        if (accountMenuButtonToggle) {
          accountMenuButtonToggle.addEventListener('click', function () {
            toggleAccountMenu();
          });
        }
        /**
         * When click outside opened accountMenu, close it
         */


        if (accountMenu && accountMenuButtonToggle) {
          document.addEventListener('click', function (event) {
            // Mandatory test: Otherwise !isClickInsideAccountMenu is already
            // true when opening
            var isClickInsideAccountMenuButtonToggle = accountMenuButtonToggle.contains(event.target);

            if (!isClickInsideAccountMenuButtonToggle) {
              if (accountMenu.classList.contains('js-header__account-menu--opened')) {
                var isClickInsideAccountMenu = accountMenu.contains(event.target);

                if (!isClickInsideAccountMenu) {
                  toggleAccountMenu();
                }
              }
            }
          });
        }
      }); // Init Responsive
      // If desktop and up

      enquire.register('screen and (min-width: 992px)', {
        match: function match() {
          // Reset mobile header on desktop
          if (htmlElement.classList.contains('js-html-header--opened')) {
            toggleHeader();
          }
        }
      });
      once('footer', '.footer').forEach(function (element) {
        var footerPopin = document.getElementById('js-footer__actions-popin');
        var footerPopinOverlay = document.getElementById('js-footer__actions-popin-overlay');
        var footerPopinButtonOpen = document.getElementById('js-footer__actions-popin-button-open');
        var footerPopinButtonClose = document.getElementById('js-footer__actions-popin-button-close');
        var isFooterPopinOpened = false; // BEGIN TODO REMOVE temp condition

        function toggleFooterPopin() {
          isFooterPopinOpened = !isFooterPopinOpened;

          if (footerPopinButtonOpen) {
            footerPopinButtonOpen.setAttribute("aria-expanded", isFooterPopinOpened);
          }

          if (footerPopinButtonClose) {
            footerPopinButtonClose.setAttribute("aria-expanded", isFooterPopinOpened);
          }

          footerPopin.classList.toggle('js-footer__actions-popin--opened');
          htmlElement.classList.toggle('js-html--no-scroll');
        }

        if (footerPopinButtonOpen) {
          footerPopinButtonOpen.addEventListener('click', function () {
            toggleFooterPopin();
          });
        }

        if (footerPopinButtonClose) {
          footerPopinButtonClose.addEventListener('click', function () {
            toggleFooterPopin();
          });
        }

        if (footerPopinOverlay) {
          footerPopinOverlay.addEventListener('click', function () {
            toggleFooterPopin();
          });
        }
      });
    }
  };
})(jQuery, Drupal, once);

