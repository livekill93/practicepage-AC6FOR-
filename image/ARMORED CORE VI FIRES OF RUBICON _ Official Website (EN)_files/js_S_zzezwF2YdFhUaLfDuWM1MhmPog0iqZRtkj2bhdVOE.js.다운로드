var cookiePolicyUrl = '/cookies-policy#q3';

// de script
tarteaucitron.services.scriptde = {
  key: 'scriptde',
  type: 'analytic',
  name: 'DE Campaign',
  needConsent: true,
  cookies: ['__cfduid', 'x_qtag_*'],
  uri: cookiePolicyUrl,
  js: function () {
    'use strict';
    tarteaucitron.addScript(
      '//grmtech.net/r/dedc16622ddc767e6bc1200fe5df2fbdfb.js'
    );
  },
  fallback: function () {
    'use strict';
  },
};

// youtubeapi BNEE
/**
 * Custom service used to improve tarteaucitron.services.youtubeapi:
 * - Fix cookies
 * - Update call to YouTube IFrame Player API
 * - Add fallback to use with HTML 'youtube_api_bnee' class name
 * @type {{needConsent: boolean, name: string, js: tarteaucitron.services.youtubeapibnee.js, type: string, uri: string, fallback: tarteaucitron.services.youtubeapibnee.fallback, key: string, cookies: string[]}}
 */
tarteaucitron.services.youtubeapibnee = {
  "key": "youtubeapibnee",
  "type": "video",
  "name": "Youtube (Js API)",
  "uri": "https://policies.google.com/privacy/",
  "needConsent": true,
  "cookies": [
    'ANID',
    'CONSENT',
    'IDE',
    'NID',
    'VISITOR_INFO1_LIVE',
    'YSC'
  ],
  "js": function () {
    "use strict";
    tarteaucitron.addScript('https://www.youtube.com/iframe_api');
    tarteaucitron.fallback(['youtube_api_bnee'], function (x) {

      // @TODO: Factorisable? > Only status changes
      var statusServiceYoutubeapibnee = new CustomEvent('tarteaucitron_youtubeapibnee_allowed', {
        detail: {
          status: true
        }
      });
      document.dispatchEvent(statusServiceYoutubeapibnee);

    });
  },
  "fallback": function () {
    "use strict";
    var id = 'youtubeapibnee';
    tarteaucitron.fallback(['youtube_api_bnee'], function (elem) {

      // @TODO: Factorisable? > Only status changes
      var statusServiceYoutubeapibnee = new CustomEvent('tarteaucitron_youtubeapibnee_allowed', {
        detail: {
          status: false
        }
      });
      document.dispatchEvent(statusServiceYoutubeapibnee);

      elem.style.width = elem.getAttribute('width') + 'px';
      elem.style.height = elem.getAttribute('height') + 'px';
      return tarteaucitron.engage(id);
    });
  }
};

// Push new youtubeapibnee custom service
(tarteaucitron.job = tarteaucitron.job || []).push('youtubeapibnee');

function delete_cookie(name) {
  document.cookie = name + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}
